{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.1.1",
  "homepage_url": "https://github.com/mikl/add-to-omnifocus-webextension",
  "default_locale": "en",
  "icons": {
    "48": "assets/omnifocus-48.png",
    "96": "assets/omnifocus-96.png"
  },
  "permissions": [
    "activeTab",
    "menus"
  ],
  "browser_action": {
    "default_icon": {
      "16": "assets/omnifocus-16.png",
      "32": "assets/omnifocus-32.png",
      "64": "assets/omnifocus-64.png"
    },
    "default_title": "__MSG_extensionName__"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ]
  }
}
